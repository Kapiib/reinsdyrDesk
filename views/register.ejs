<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head')  %>
<body>
    <%- include('partials/navbar')  %>

    <h1>Welcome to <%= title %></h1>

    <% if (typeof msg !== 'undefined') { %>
        <div style="color: red;"> <%= msg %> </div>
    <% } %>

    <form id="register" action="/auth/register" method="POST">
        <label for="username">Username</label>
        <input type="text" id="navn" name="navn" >

        <label for="epost">Email</label>
        <input type="email" id="epost" name="epost" >

        <label for="kontaktsprak">kontaktsprak</label>
        <input type="text" id="kontaktsprak" name="kontaktsprak" >

        <label for="telefonnummer">telefonnummer</label>
        <input type="number" id="telefonnummer" name="telefonnummer" >

        <label for="password">Password</label>
        <input type="password" id="passord" name="passord" >

        <label for="Rpassword">Repeat Password</label>
        <input type="password" id="Rpassord" name="Rpassord">

        <button>Submit</button>
    </form>
    
</body>
</html>